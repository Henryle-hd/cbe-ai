generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


//CBE information
model Info{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  title String
  main_body String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("info")
}


//noted prompt enter by user
model Prompt{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  prompt String
  createdAt DateTime @default(now())

  @@map("prompt")
}